@model List<WebAppQueueManagmentSystem.ApiHelpers.Response.UserListBody>


<table id="example2" class="table table-striped table-bordered second" style="width:100%">
    <thead>
        <tr>
            <th>Email</th>
            <th>Role</th>
            @*<th>Action</th>*@
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Email
                </td>
                <td>
                    <span class="badge badge-success">@item.Role</span>
                </td>
                @*<td>
                    <a href="#" class="btn btn-primary">
                        <i class="fas fa-edit"></i>
                        Edit
                    </a>
                </td>*@
            </tr>
        }


    </tbody>


</table>